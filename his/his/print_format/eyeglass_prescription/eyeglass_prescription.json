 "doc_type": "EyeGlass Prescription",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<main class=\"mx\">\n        <header style=\"display: flex; align-items: center; gap: 50px; border-bottom: 2px solid black;\" class=\"mx\">\n        <div>\n           {{ letter_head }}\n        </div>\n    </header>\n    \n    <h1 style=\"text-decoration: underline; text-align: center;\">EYE GLASSES PRESCRIPTION</h1>\n    \n          <div style=\"display: flex; flex-direction: column; gap: 20px; margin-bottom: 20px\">\n                 <div>\n            <span style=\"font-size: 18px;\"><strong>PID</strong>: <span style=\"color: black; font-size: 18px\">{{doc.patient}}</span></span>\n        </div>\n                    <div>\n            <span style=\"font-size: 18px;\"><strong>Date</strong>: <span style=\"color: black; font-size: 18px\">{{doc.date}}</span></span>\n        </div>\n        <div>\n            <span style=\"font-size: 18px;\"><strong>Name</strong>: <span style=\"color: black; font-size: 18px\">{{doc.patient_name}}</span></span>\n        </div>\n      </div>\n    \n    <div>\n        <table class=\"table1\">\n            <thead>\n                <tr>\n                    <th style=\"color: black; font-size: 18px\">Lenses material</th>\n                    <th style=\"color: black; font-size: 18px\">Lenses advised</th>\n                    <th style=\"color: black; font-size: 18px\">Specification</th>\n                </tr>\n            </thead>\n                        <tbody>\n                <tr>\n                <td style=\"font-size: 18px\">{{doc.lenes_material}}</td>\n            <!--Lenses advised-->\n                <td style=\"font-size: 18px\">\n                    {% if doc.distance or doc.near or doc.bifocal or doc.progressive or doc.sunglass or doc.contac_lenses %}\n                        {% if doc.distance %}\n                            Distance,\n                        {% endif %}\n                        {% if doc.near %}\n                            Near,\n                        {% endif %}\n                        {% if doc.bifocal %}\n                            Bifocal,\n                        {% endif %}\n                        {% if doc.progressive %}\n                            Progressive,\n                            <br>\n                        {% endif %}\n                        {% if doc.sunglass %}\n                            Sunglass,\n                        {% endif %}\n                        {% if doc.contac_lenses %}\n                            Contact lenses\n                        {% endif %}\n                    {% else %}\n                        No lenses advised\n                    {% endif %}\n                </td>\n            <!--Lenses advised-->\n                \n            <!--specification-->\n                    <td style=\"font-size: 18px\">\n                        {% if doc.photo_solar or doc.balance or doc.seperate or doc.ar or doc.hc_coating or doc.hmc_coating or doc.polorized_white or doc.white %}\n                            {% if doc.photo_solar %}\n                                Photo Solar,\n                            {% endif %}\n                            {% if doc.balance %}\n                                Balance,\n                            {% endif %}\n                            {% if doc.seperate %}\n                                Separate,\n                            {% endif %}\n                            {% if doc.ar %}\n                                AR (Blue Cut),\n                            {% endif %}\n                            {% if doc.hc_coating %}\n                                HC Coating,\n                                <br>\n                            {% endif %}\n                            {% if doc.hmc_coating %}\n                                HMC Coating,\n                            {% endif %}\n                            {% if doc.polorized_white %}\n                                Polarized White,\n                            {% endif %}\n                            {% if doc.white %}\n                                White\n                            {% endif %}\n                        {% else %}\n                            No specifications\n                        {% endif %}\n                    </td>\n                <!--specification-->\n                </tr>\n            </tbody>\n        </table>\n        \n    <!--     <table class=\"info-table\">-->\n    <!--    <tr>-->\n    <!--        <th style=\"color: black\">Tinted</th>-->\n    <!--        <td>{{doc.tnted}}</td>-->\n    <!--    </tr>-->\n    <!--    <tr>-->\n    <!--        <th style=\"color: black\">Diameter</th>-->\n    <!--        <td>{{doc.diameter}}</td>-->\n    <!--    </tr>-->\n    <!--</table>-->\n    \n    <div style=\"display: flex; flex-direction: column; gap: 10px\">\n        \n        <!--right-->\n        <div style=\"width: 100%\">\n            <h3>Right Eye</h3>\n            <table class=\"table1\" style=\"width: 100%\">\n                <thead>\n                    <tr>\n                        <th style=\"color: black; font-size: 18px;\">UCVA</th>\n                        <th style=\"color: black; font-size: 18px;\">Sph</th>\n                        <th style=\"color: black; font-size: 18px;\">Cyl</th>\n                        <th style=\"color: black; font-size: 18px;\">Axis</th>\n                        <th style=\"color: black; font-size: 18px;\">ADD</th>\n                        <th style=\"color: black; font-size: 18px;\">PRISMA</th>\n                        <th style=\"color: black; font-size: 18px;\">CV/A</th>\n                    </tr>\n                </thead>\n                <tbody>\n                {% for d in doc.right_eye %}\n                    <tr>\n                        <td style=\"font-size: 18px;\">{{d.ucva or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.sph or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.cyl or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.axis or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.add or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.prisma or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.s_va or ''}}</td>\n                    </tr>\n                \n                {% endfor %}\n                </tbody>\n            </table>\n        </div>\n        \n        <!--left-->\n        <div style=\" width: 100%\">\n            <h3>Left Eye</h3>\n            <table class=\"table1\" style=\"width: 100%\">\n                <thead>\n                    <tr>\n                          <th style=\"color: black; font-size: 18px;\">UCVA</th>\n                        <th style=\"color: black; font-size: 18px;\">Sph</th>\n                        <th style=\"color: black; font-size: 18px;\">Cyl</th>\n                        <th style=\"color: black; font-size: 18px;\">Axis</th>\n                        <th style=\"color: black; font-size: 18px;\">ADD</th>\n                        <th style=\"color: black; font-size: 18px;\">PRISMA</th>\n                        <th style=\"color: black; font-size: 18px;\">CV/A</th>\n                    </tr>\n                </thead>\n                <tbody>\n                {% for d in doc.left_eye %}\n                    <tr>\n                        <td style=\"font-size: 18px;\">{{d.ucva or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.sph or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.cyl or \"\"}}</td>\n                        <td style=\"font-size: 18px;\">{{d.axis or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.add or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.prisma or ''}}</td>\n                        <td style=\"font-size: 18px;\">{{d.s_va or ''}}</td>\n                    </tr>\n                \n                {% endfor %}\n                </tbody>\n            </table>\n        </div>\n    </div>\n\n    </div>\n    \n    <div style=\"display: flex; flex-direction: column; gap: 5px; font-size: 18px; margin-top: 14px\">\n        <span style=\"font-size: 18px\">Distance</span>\n        <span style=\"font-size: 18px\"><stron>P.D:</stron> <span style=\"color: black; font-size: 18px\">{{doc.distance_pd or \"\"}}</span></span>\n         <span style=\"font-size: 18px\"><stron>Near P.D:</stron> <span style=\"color: black; font-size: 18px\">{{doc.near_pd}}</span></span>\n          <span style=\"font-size: 18px\"><stron>Note:</stron> <span style=\"color: black; font-size: 18px\">{{doc.note}}</span></span>\n    </div>\n    \n     <div class=\"row\">\n          \n        <div class=\"col-xs-3 text-left\" style=\"color: black; font-size: 18px\">\n         Refered:\n        </div>\n        {% if doc.practitioner %}\n        <div class=\"col-xs-9  text-left value\" style=\"color: black; font-size: 18px\">\n         <label style=\"font-size: 18px\">  {{ doc.practitioner }} </label> \n        </div>\n        {% else %}\n\t\t\t{% if doc.practitioner %}\n            <div style=\"font-size: 18px\" class=\"col-xs-10  text-left value\">\n            {{ doc.practitioner }}\n            </div>\n\t\t    {% endif %}\n        {%- endif -%}\n      </div>\n      <p style=\"font-size: 18px\">Printed By: {{frappe.db.get_value(\"User\" , doc.owner , \"full_name\")}}</p>\n      <!--<span class=\"span\">{{frappe.db.get_value(\"User\" , doc.owner , \"full_name\")}}</span>-->\n      <span style=\"color: black; font-size: 18px\"><stron>Signature:</stron> </span>\n    \n    \n</main>",
